{
	"ProductAPI": {
		"entity": "Products",
		"functions": [{
				"name": "getProductByID",
				"type": "getbyid"
			},
			{
				"name": "getProductsWithPriceHigherThan",
				"type": "query",
				"return": "iterator",
				"filters": [{
					"name":"minPrice",
					"property": "UnitPrice",
					"operator": "gt"
				},
				{ 
					"name":"maxPrice",
					"property": "UnitPrice",
					"operator": "lt"
				}]
			},
			{
				"name": "getProductsWithName",
				"type": "query",
				"filters": [{
					"property": "ProductName",
					"operator": "like"
				}]
			}
		]
	}
}